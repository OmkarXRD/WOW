                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingHorizontal="20dp">


                        <ImageView
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:src="@drawable/family" />

                        <TextView
                            android:id="@+id/familyTxt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20dp"
                            android:text="Family"
                            android:textColor="@color/black"
                            android:textSize="16sp"
                            android:letterSpacing="0.08"
                            android:textStyle="bold" />

                    </LinearLayout>

                    <RelativeLayout
                        android:id="@+id/onlineRL"
                        android:layout_width="match_parent"
                        android:layout_height="65dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingHorizontal="20dp">


                        <ImageView
                            android:id="@+id/onlineServiceImg"
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:src="@drawable/internnet"
                            app:tint="#FFA500" />

                        <TextView
                            android:id="@+id/online"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20dp"
                            android:layout_toRightOf="@+id/onlineServiceImg"
                            android:letterSpacing="0.08"
                            android:text="Online Services"
                            android:textColor="@color/black"
                            android:textSize="16sp"
                            android:textStyle="bold" />

                        <ImageView
                            android:layout_width="16dp"
                            android:layout_height="16dp"
                            android:layout_alignParentEnd="true"
                            android:layout_gravity="center_vertical|center"
                            android:layout_marginStart="10dp"
                            android:rotation="180"
                            android:src="@drawable/back__1_"
                            app:tint="@color/grey" />


                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/settingsRL"
                        android:layout_width="match_parent"
                        android:layout_height="65dp"
                        android:layout_marginBottom="70dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingHorizontal="20dp">


                        <ImageView
                            android:id="@+id/settingImg"
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:src="@drawable/setttings" />

                        <TextView
                            android:id="@+id/settings"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20dp"
                            android:layout_toRightOf="@+id/settingImg"
                            android:letterSpacing="0.08"
                            android:text="Settings"
                            android:textColor="@color/black"
                            android:textSize="16sp"
                            android:textStyle="bold" />

                        <ImageView
                            android:layout_width="16dp"
                            android:layout_height="16dp"
                            android:layout_alignParentEnd="true"
                            android:layout_gravity="center_vertical|center"
                            android:layout_marginStart="10dp"
                            android:rotation="180"
                            android:src="@drawable/back__1_"
                            app:tint="@color/grey" />

                    </RelativeLayout>
                </LinearLayout>

            </RelativeLayout>
        </androidx.core.widget.NestedScrollView>

    </RelativeLayout>

</RelativeLayout>
